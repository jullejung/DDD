<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="LoginMapper">

	<select id="login" parameterType="kr.co.DDD.util.dto.ProjectUserDTO"
						resultType="kr.co.DDD.util.dto.ProjectUserDTO">
	select user_no, user_id, user_name, phone, user_date
	from user
	where mid = #{user_id} and user_pwd = #{user_pwd}
	</select>

</mapper>


